{
  "AllowedHosts": "*",
  "CacheOptions": {
    "ExpirationInMinutes": 10
  },
  "ConnectionStrings": {
    "QuizGame": "Server=[Server];Database=QuizGame;User ID=[User];Password=[Password];Encrypt=True;Trust Server Certificate=True;MultipleActiveResultSets=True;"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5283"
      },
      "Https": {
        "Url": "https://localhost:7083"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting": "Information"
    }
  }
}
